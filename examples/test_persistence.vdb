# Create test dataset
DATASET test_users COLUMNS (id: INT, name: STRING, score: FLOAT)

# Insert some data
INSERT INTO test_users VALUES (1, "Alice", 95.5)
INSERT INTO test_users VALUES (2, "Bob", 88.0)
INSERT INTO test_users VALUES (3, "Charlie", 92.0)

# Show the dataset
SELECT * FROM test_users

# Save to disk (relative path)
SAVE DATASET test_users TO "./linal_data"

# List saved datasets
LIST DATASETS FROM "./linal_data"

# List saved tensors (will be empty effectively until SAVE TENSOR is enabled)
LIST TENSORS FROM "./linal_data"
